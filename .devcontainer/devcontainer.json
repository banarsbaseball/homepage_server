{
    "name": "remote-java",
    "dockerComposeFile": "../docker/docker-compose.yml",
    "service": "java",
    "workspaceFolder": "/app",
    "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
    },
    "extensions": [
      "vscjava.vscode-java-pack",
      "pivotal.vscode-boot-dev-pack",
      "gabrielbb.vscode-lombok"
    ],
    "postCreateCommand": "apt update && apt install -y git && git clone -b master https://github.com/banarsbaseball/homepage_server.git",
  }
  